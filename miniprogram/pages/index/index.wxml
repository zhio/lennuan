<!--index.wxml-->
<view class="page">
    <view class="page__hd">
        <view class="page__title">SearchBar</view>
        <view class="page__desc">搜索栏</view>
    </view>
    <view class="page__bd">
        <mp-searchbar bindselectresult="selectResult" search="{{search}}"></mp-searchbar>
    </view>
    <view class="container">
      <view class="uploader">
        <navigator url="../today/today" open-type="navigate" class="uploader-text">
          <text>历史上的今天</text>
        </navigator>
      </view>
    </view>
    <view class="container">
      <view class="uploader">
        <navigator url="../lastyear/lastyear" open-type="navigate" class="uploader-text">
          <text>去年今日</text>
        </navigator>
      </view>
    </view>

</view>
